{
    "family": "node-task-ec2", 
    "networkMode": "bridge", 
    "executionRoleArn": "arn:aws:iam::368355641188:role/ecsTaskExecutionRole",
    "containerDefinitions": [
        {
            "name": "node-container", 
            "image": "368355641188.dkr.ecr.us-east-1.amazonaws.com/node-app:tag_value", 
            "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "node-log-tf",
        "awslogs-region": "ap-northeast-1",
        "awslogs-stream-prefix": "node-tf"
      }
    },
            "portMappings": [
                {
                    "containerPort": 3000, 
                    "hostPort": 0, 
                    "protocol": "tcp"
                }
            ], 
            "essential": true
        }
    ], 
    "requiresCompatibilities": [
        "EC2"
    ], 
    "cpu": "1 vcpu", 
    "memory": "1000"
}
